        "#     trainer = WordLevelTrainer(special_tokens=[\"[UNK]\", \"[PAD]\", \"[SOS]\", \"[EOS]\"], min_frequency = 2)\n",
        "\n",
        "#     tokenizer.train_from_iterator(get_all_sentences(ds, lang), trainer=trainer)\n",
        "#     print(tokenizer)\n",
